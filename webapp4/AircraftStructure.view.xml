	"ui5/rest/restwithui5/view/Booking.view.xml":'<mvc:View\n    xmlns:f="sap.f"\n    controllerName="ui5.rest.restwithui5.controller.Booking"\n    xmlns:mvc="sap.ui.core.mvc"\n    xmlns="sap.m"\n    xmlns:c="sap.ui.core"\n    displayBlock="true"\n    height="100%"\n    busyIndicatorDelay="0"\n><f:DynamicPage\n        backgroundDesign="List"\n        fitContent="false"\n        headerExpanded="true"\n        headerPinned="false"\n        preserveHeaderStateOnScroll="false"\n        showFooter="false"\n        toggleHeaderOnTitleClick="true"\n    ><f:content><VBox\n                alignItems="Center"\n                width="100%"\n                class="sapUiLargeMarginBottom"\n            ><List\n                    backgroundDesign="Solid"\n                    items="{/bookings}"\n                    id="bookingsList"\n                ><CustomListItem><HBox\n                            class="sapUiSmallMargin gap-2 flightCard hover-shadow max-w-lg"\n                            alignItems="Center"\n                            justifyContent="SpaceAround"\n                            wrap="Wrap"\n                            width="100%"\n                        ><layoutData><FlexItemData maxWidth="747px" /></layoutData><HBox\n                                width="100%"\n                                justifyContent="SpaceBetween"\n                            ><Text text="Booking Id: {bookingId}" /><Text\n                                    text="Seats booked: {= ${bookedSeats}}"\n                                    class="mt-1"\n                                    visible="{path:\'bookedSeats\', formatter: \'.isListNotEmpty\' }"\n                                /></HBox><FlexBox\n                                class="gap-2"\n                                alignItems="Center"\n                                wrap="Wrap"\n                            ><Image\n                                    src="{flight/companyLogoUrl}"\n                                    alt="Flight_operator_logo"\n                                    width="35px"\n                                /><VBox><Text text="{flight/company}" /><Text text="{flight/flightName}" /></VBox></FlexBox><FlexBox class="gap-2"><FlexBox\n                                    direction="Column"\n                                    justifyContent="Center"\n                                    alignItems="Center"\n                                    wrap="Wrap"\n                                ><Text\n                                        text="{path: \'flight/departureDateTimeLocal\', formatter: \'.formatDate\'}"\n                                        class="size-1"\n                                    /><Label\n                                        text="{flight/sourceAirport/city} ({flight/sourceAirport/airportCode})"\n                                        class="sapUiTinyMarginTop"\n                                    /></FlexBox><FlexBox\n                                    direction="Column"\n                                    alignItems="Center"\n                                    justifyContent="Center"\n                                    height="100%"\n                                    class="sapUiTinyMarginBottom"\n                                    wrap="Wrap"\n                                ><Text\n                                        text="{parts :[\'flight/departureDateTimeLocal\', \'flight/arrivalDateTimeLocal\'], formatter: \'.getDuration\'} hrs"\n                                        class="size-1"\n                                    /><VBox\n                                        width="80px"\n                                        height="2px"\n                                        class="bg-blue mt-1"\n                                    /></FlexBox><FlexBox\n                                    direction="Column"\n                                    justifyContent="Center"\n                                    alignItems="Center"\n                                    wrap="Wrap"\n                                ><Text\n                                        text="{path: \'flight/arrivalDateTimeLocal\', formatter: \'.formatDate\'}"\n                                        class="size-1"\n                                    /><Label\n                                        text="{flight/destinationAirport/city} ({flight/destinationAirport/airportCode})"\n                                        class="sapUiTinyMarginTop"\n                                    /></FlexBox></FlexBox><FlexBox\n                                class="gap-2"\n                                wrap="Wrap"\n                            ><FlexBox\n                                    direction="Column"\n                                    alignItems="Center"\n                                ><Text\n                                        text="Rs {amount}/-"\n                                        class="size-2"\n                                    /><Label\n                                        text="Total Amount paid"\n                                        class="mt-1"\n                                    /></FlexBox></FlexBox><VBox><Button\n                                    text="View Details"\n                                    class="flightButton mt-1"\n                                    type="Emphasized"\n                                    icon="sap-icon://detail-more"\n                                    iconFirst="false"\n                                    press="onViewDetailsPress"\n                                /><Label\n                                    text="Status: {bookingStatus}"\n                                    wrapping="true"\n                                    class="mt-1"\n                                /></VBox></HBox></CustomListItem></List></VBox></f:content><f:footer /><f:header><f:DynamicPageHeader\n                backgroundDesign=""\n                pinnable="false"\n            ><f:content><HBox justifyContent="SpaceBetween"><VBox><Title text="{/user/name}\'s Bookings" /><Label\n                                text="lorem ipsum dor sit ipem"\n                                wrapping="true"\n                            /></VBox><VBox alignItems="End"><Title\n                                text="{=  ${/bookings}.length} Bookings"\n                                class="text-green"\n                            /><Label\n                                text="lorem ipsum dor sit ipem"\n                                wrapping="true"\n                            /></VBox></HBox></f:content></f:DynamicPageHeader></f:header><f:landmarkInfo /><f:title /></f:DynamicPage></mvc:View>\n',
	"ui5/rest/restwithui5/view/Detail.view.xml":'<mvc:View\n    xmlns:f="sap.f"\n    controllerName="ui5.rest.restwithui5.controller.Detail"\n    xmlns:mvc="sap.ui.core.mvc"\n    xmlns="sap.m"\n    xmlns:c="sap.ui.core"\n    displayBlock="true"\n    busyIndicatorDelay="0"\n><Page class="-mt-4"><f:DynamicPage\n            title="Flight Details"\n            showFooter="true"\n        ><f:content><VBox><VBox class=""><Title text="Select Seats" /><Label text="Add passengers for the seats you select" /><HBox\n                            width="100%"\n                            class="flightCardNoBg sapUiTinyMarginTop bg-gray gap-2"\n                            justifyContent="SpaceAround"\n                            wrap="Wrap"\n                        ><mvc:XMLView\n                                viewName="ui5.rest.restwithui5.view.AircraftStructure"\n                                class="mx-auto"\n                            /><VBox\n                                backgroundDesign="Solid"\n                                class="flightCard"\n                                width="100%"\n                            ><layoutData><FlexItemData maxWidth="560px" /></layoutData><mvc:XMLView\n                                    viewName="ui5.rest.restwithui5.view.PassengerForm"\n                                /></VBox></HBox></VBox><HBox\n                        justifyContent="SpaceBetween"\n                        wrap="Wrap"\n                        class="gap-2 flightCard sapUiSmallMarginTop sapUiLargeMarginBottom"\n                        backgroundDesign="Solid"\n                    ><VBox><Title text="Seat Booking Policy" /><Label\n                                text="All the seats are priced equally and are priced per person."\n                                class="max-w-xs"\n                                wrapping="true"\n                            /></VBox><VBox><Title text="Seat Cancellation Policy" /><Label\n                                text="Seats once booked can be cancelled with a 25% cancellation penalty per seat."\n                                class="max-w-xs"\n                                wrapping="true"\n                            /></VBox></HBox></VBox></f:content><f:footer><OverflowToolbar><ToolbarSpacer /><Text\n                        text="{/flight/price} x {= ${/passengersCount} ? ${/passengersCount} : 0}"\n                    /><Text text="=" /><Button\n                        type="Accept"\n                        text="Pay Rs {= %{/passengersCount} ? ${/passengersCount} * %{/flight/price} : 0 } /-"\n                        press="payAmountButtonPress"\n                    /></OverflowToolbar></f:footer><f:header><f:DynamicPageHeader pinnable="false"><f:content><VBox><Title text="Flight Details" /><Label\n                                text="{/flight/flightName} Flight from {/flight/sourceAirport/city} to {/flight/destinationAirport/city}"\n                            /></VBox><VBox class="sapUiMediumMarginTop"><HBox\n                                alignItems="Center"\n                                wrap="Wrap"\n                                justifyContent="SpaceAround"\n                                class="gap-2"\n                            ><HBox\n                                    class="gap-2"\n                                    alignItems="Center"\n                                ><Image\n                                        src="{/flight/companyLogoUrl}"\n                                        alt="Company logo"\n                                        height="50px"\n                                    /><VBox><Title text="{/flight/company}" /><Label\n                                            text="{= ${/flight/flightName}}"\n                                        /></VBox></HBox><HBox\n                                    class="gap-2"\n                                    alignItems="Center"\n                                ><c:Icon\n                                        src="sap-icon://flight"\n                                        size="30px"\n                                        class="rotate-45"\n                                    /><VBox><Title\n                                            text="{/flight/departureDateTimeLocalTimeStamp}"\n                                        /><Text\n                                            text="{/flight/sourceAirport/airportName} ({/flight/sourceAirport/airportCode})"\n                                        /><Label\n                                            text="{/flight/sourceAirport/city}, {/flight/sourceAirport/country}"\n                                        /></VBox></HBox><VBox\n                                    alignItems="Center"\n                                    justifyContent="Center"\n                                    class=" gap-1"\n                                ><Text text="Duration: 03:30 hrs" /><VBox\n                                        width="10rem"\n                                        height="2px"\n                                        class="bg-blue"\n                                    /><Text\n                                        text="{path: \'/flight/availableSeats\', formatter: \'.length\'} / {/flight/seats} seats left"\n                                    /></VBox><HBox\n                                    class="gap-2"\n                                    alignItems="Center"\n                                ><c:Icon\n                                        src="sap-icon://flight"\n                                        size="30px"\n                                        class="rotate-120"\n                                    /><VBox><Title\n                                            text="{/flight/arrivalDateTimeLocalTimeStamp}"\n                                        /><Text\n                                            text="{/flight/destinationAirport/airportName} ({/flight/destinationAirport/airportCode})"\n                                        /><Label\n                                            text="{/flight/destinationAirport/city}, {/flight/destinationAirport/country}"\n                                        /></VBox></HBox><VBox class=""><Title text="Baggage for this Itenary" /><Text text="Check In: 15kg / person" /><Text text="Cabin: 7kg / person" /></VBox></HBox></VBox></f:content></f:DynamicPageHeader></f:header><f:landmarkInfo /><f:title /></f:DynamicPage></Page></mvc:View>\n',
	"ui5/rest/restwithui5/view/FlightCard.view.xml":'<mvc:View\n    controllerName="ui5.rest.restwithui5.controller.FlightCard"\n    xmlns:mvc="sap.ui.core.mvc"\n    xmlns="sap.m"\n    xmlns:f="sap.f"\n    xmlns:layout="sap.ui.layout"\n    xmlns:card="sap.f.cards"\n    height="100%"\n    xmlns:l="sap.ui.layout"\n><HBox\n        class="sapUiSmallMargin gap-2 flightCard"\n        alignItems="Center"\n        justifyContent="SpaceAround"\n        wrap="Wrap"\n        width="100%"\n    ><layoutData><FlexItemData maxWidth="747px" /></layoutData><FlexBox\n            class="gap-2"\n            alignItems="Center"\n            wrap="Wrap"\n        ><Image\n                src="https://imgak.mmtcdn.com/flights/assets/media/dt/common/icons/6E.png?v=19"\n                alt="Flight_operator_logo"\n                width="35px"\n            /><VBox><Text text="INDIGO" /><Text text="BLR-DEL-1" /></VBox></FlexBox><FlexBox class="gap-2"><FlexBox\n                direction="Column"\n                justifyContent="Center"\n                alignItems="Center"\n                wrap="Wrap"\n            ><Text\n                    text="18:15"\n                    class="size-2"\n                /><Label\n                    text="New Delhi"\n                    class="size-1 mt-1"\n                /></FlexBox><FlexBox\n                direction="Column"\n                alignItems="Center"\n                justifyContent="Center"\n                height="100%"\n                class="sapUiTinyMarginBottom"\n                wrap="Wrap"\n            ><Text text="01:30 hrs" /><VBox\n                    width="80px"\n                    height="2px"\n                    class="bg-blue mt-1"\n                /></FlexBox><FlexBox\n                direction="Column"\n                justifyContent="Center"\n                alignItems="Center"\n                wrap="Wrap"\n            ><Text\n                    text="18:15"\n                    class="size-2"\n                /><Label\n                    text="New Delhi"\n                    class="size-1 mt-1"\n                /></FlexBox></FlexBox><FlexBox\n            class="gap-2"\n            wrap="Wrap"\n        ><FlexBox\n                direction="Column"\n                alignItems="Center"\n            ><Text\n                    text="Rs 3000.5"\n                    class="size-2"\n                /><Label\n                    text="per person"\n                    class="mt-1"\n                /></FlexBox><VBox><Button\n                    text="Book Seats"\n                    class="flightButton"\n                /></VBox></FlexBox></HBox></mvc:View>\n',
	"ui5/rest/restwithui5/view/Form.view.xml":'<mvc:View\n    controllerName="ui5.rest.restwithui5.controller.Form"\n    xmlns:mvc="sap.ui.core.mvc"\n    xmlns="sap.m"\n    xmlns:c="sap.ui.core"\n    displayBlock="true"\n    busyIndicatorDelay="0"\n><VBox\n        class="mx-auto"\n        width="100%"\n        backgroundDesign="Solid"\n    ><Title\n            text="{data>/greetings} Traveler, begin your search below"\n            class="capitalizeText sapUiTinyMarginBottom"\n        /><mvc:XMLView viewName="ui5.rest.restwithui5.view.Underline" /><HBox\n            class="sapUiMediumMarginTop sapUiSmallMarginBottom gap-2 "\n            justifyContent="Center"\n            backgroundDesign="Solid"\n            wrap="Wrap"\n            width="100%"\n        ><VBox><Label text="Source" /><SearchField\n                    value="{= (${/srcAirport/airportName}) ? ${/srcAirport/city} + \' (\' + ${/srcAirport/airportCode} + \')\' : \'\'}"\n                    showSearchButton="false"\n                    id="searchSrcAirport"\n                    placeholder="Search Airport"\n                    enableSuggestions="true"\n                    suggest=".onSuggest"\n                    suggestionItems="{\n\t\t\t\tpath: \'/suggestedAirports\',\n\t\t\t\tsorter: { path: \'city\' }\n\t\t\t}"\n                    width="100%"\n                ><layoutData><FlexItemData maxWidth="400px" /></layoutData><SuggestionItem\n                        key="{airportCode}"\n                        text="{city} ({airportCode})"\n                        description="{airportName}"\n                    /></SearchField></VBox><VBox><Label text="Destination" /><SearchField\n                    value="{= (${/destAirport/airportName}) ? ${/destAirport/city} + \' (\' + ${/destAirport/airportCode} + \')\' : \'\'}"\n                    showSearchButton="false"\n                    id="searchDestAirport"\n                    placeholder="Search Airport"\n                    enableSuggestions="true"\n                    suggest=".onSuggest"\n                    suggestionItems="{\n\t\t\t\tpath: \'/suggestedAirports\',\n\t\t\t\tsorter: { path: \'city\' }\n\t\t\t}"\n                    width="100%"\n                ><SuggestionItem\n                        key="{airportCode}"\n                        text="{city} ({airportCode})"\n                        description="{airportName}"\n                    /></SearchField></VBox><VBox width="100%"><layoutData><FlexItemData maxWidth="200px" /></layoutData><Label text="Date" /><DatePicker\n                    value="{\n\t\t\t\tpath: \'/date\',\n\t\t\t\ttype: \'sap.ui.model.type.Date\',\n\t\t\t\tformatOptions: {\n                    style: \'short\',\n\t\t\t\t\tsource: {pattern: \'yyyy-MM-dd\'}\n\t\t\t\t}}"\n                    id="departureDate"\n                /></VBox><VBox\n                justifyContent="End"\n                width="100%"\n            ><layoutData><FlexItemData maxWidth="150px" /></layoutData><Button\n                    text="Search Flights"\n                    iconFirst="false"\n                    icon="sap-icon://search"\n                    type="Emphasized"\n                    width="100%"\n                    press="onSearchFlightsButtonPress"\n                /></VBox></HBox></VBox></mvc:View>\n',
	"ui5/rest/restwithui5/view/Initial.view.xml":'<mvc:View\n    controller="ui5.rest.restwithUi5.controller.Initial"\n    xmlns:mvc="sap.ui.core.mvc"\n    xmlns="sap.m"\n    xmlns:c="sap.ui.core"\n    displayBlock="true"\n    busyIndicatorDelay="0"\n><Page class="-mt-4"><content><VBox\n                backgroundDesign="Solid"\n                class="p-2"\n            ><mvc:XMLView viewName="ui5.rest.restwithui5.view.Form" /></VBox><mvc:XMLView viewName="ui5.rest.restwithui5.view.Underline" /></content><VBox class="mx-auto"><mvc:XMLView viewName="ui5.rest.restwithui5.view.About" /></VBox></Page></mvc:View>\n',
	"ui5/rest/restwithui5/view/Login.view.xml":'<mvc:View\n    xmlns:f="sap.ui.layout.form"\n    controllerName="ui5.rest.restwithui5.controller.Login"\n    xmlns:mvc="sap.ui.core.mvc"\n    xmlns="sap.m"\n    xmlns:core="sap.ui.core"\n    displayBlock="true"\n    height="100%"\n    busyIndicatorDelay="0"\n    xmlns:l="sap.ui.layout"\n><Page class="-mt-4"><content><VBox><f:SimpleForm\n                    id="LoginForm"\n                    editable="true"\n                    title="Login to Proceed"\n                    class="sapUiSmallMarginTop max-w-sm mx-auto flightCard"\n                ><f:content><VBox class="max-w-xs mx-auto"><VBox><Label text="Username" /><Input\n                                    id="name"\n                                    value="{/username}"\n                                    showClearIcon="true"\n                                    name="username"\n                                    required="true"\n                                /></VBox><VBox class="sapUiSmallMarginTop"><Label text="Password" /><Input\n                                    value="{/password}"\n                                    type="Password"\n                                    change="onStreetInputChange"\n                                    name="password"\n                                    showClearIcon="true"\n                                    required="true"\n                                /></VBox><CheckBox\n                                text="By continuing to our site you agree to our terms and conditions."\n                                class="sapUiTinyMarginTop"\n                                selected="true"\n                                wrapping="true"\n                                valueState="Information"\n                                name="terms"\n                                enabled="false"\n                            /><Button\n                                activeIcon=""\n                                ariaHasPopup="None"\