<mvc:View
    controller="ui5.rest.restwithUi5.controller.Initial"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:c="sap.ui.core"
    displayBlock="true"
    busyIndicatorDelay="0"
>
    <Page class="-mt-4">
        <content>
            <VBox
                backgroundDesign="Solid"
                class="p-2"
            >
                <mvc:XMLView viewName="ui5.rest.restwithui5.view.Form" />
            </VBox>
            <mvc:XMLView viewName="ui5.rest.restwithui5.view.Underline" />
        </content>
        <VBox class="mx-auto">
            <mvc:XMLView viewName="ui5.rest.restwithui5.view.About" />
        </VBox>
    </Page>
</mvc:View>
